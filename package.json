    "@aws-sdk/client-s3": "3.282.0",
    "@aws-sdk/client-sfn": "3.282.0",
    "@aws-sdk/client-sqs": "3.282.0",
    "@aws-sdk/lib-dynamodb": "3.282.0",
    "@aws-sdk/lib-storage": "3.282.0",
    "@aws-sdk/s3-request-presigner": "3.282.0",
    "@aws-sdk/smithy-client": "^3.0.0",
    "@aws-sdk/types": "^3.0.0",
    "aws-sdk": "^2.1333.0",
    "axios": "1.3.4",
    "crypto-random-string": "2.0.0",
    "date-fns": "2.29.3",
    "fastify": "4.9.0",
    "ioredis": "5.2.3",
    "jimp": "0.22.7",
    "js-yaml": "4.1.0",
    "jsonpath": "1.1.1",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "nanoid": "3.2.0",
    "sharp": "^0.30.6",
    "ua-parser-js": "1.0.32",
    "ulid": "2.3.0",
    "uuid": "9.0.0",
    "velocityjs": "2.0.3",

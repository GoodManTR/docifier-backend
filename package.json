{
  "name": "docifier",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "deploy": "npm run build && cdk deploy --profile bahadir --require-approval never",
    "clean": "rm -rf dist coverage tmp temp cdk.out build/tmp build/indexTmp",
    "cdk": "cdk",
    "copy-templates": "find src/project/classes -name 'template.yml' -exec sh -c 'mkdir -p dist/$(dirname ${1#src/}) && cp $1 dist/$(dirname ${1#src/})' sh {} \\;",
    "create-layer": "rm -rf temp/ && mkdir -p temp/nodejs && cp app-package.json ./temp/nodejs/package.json && npm install --silent --prod --prefix ./temp/nodejs && cd temp && zip -r ./layer.zip nodejs"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "3.374.0",
    "@aws-sdk/client-cloudfront": "3.465.0",
    "@aws-sdk/client-dynamodb": "3.465.0",
    "@aws-sdk/client-lambda": "3.465.0",
    "@aws-sdk/client-s3": "3.465.0",
    "@aws-sdk/client-sfn": "3.465.0",
    "@aws-sdk/client-sqs": "3.465.0",
    "@aws-sdk/lib-dynamodb": "3.465.0",
    "@aws-sdk/lib-storage": "3.465.0",
    "@aws-sdk/s3-request-presigner": "3.465.0",
    "@aws-sdk/smithy-client": "3.374.0",
    "@aws-sdk/types": "3.465.0",
    "@aws-sdk/util-dynamodb": "3.465.0",
    "aws-sdk": "2.1335.0",
    "axios": "1.3.4",
    "crypto-random-string": "2.0.0",
    "date-fns": "2.29.3",
    "dot-prop": "6.0.1",
    "fastify": "4.9.0",
    "firebase-admin": "11.9.0",
    "fuse.js": "6.6.2",
    "ioredis": "5.2.3",
    "jimp": "0.22.7",
    "js-yaml": "4.1.0",
    "jsonpath": "1.1.1",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "nanoid": "3.2.0",
